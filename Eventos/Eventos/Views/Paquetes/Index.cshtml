@model IEnumerable<Eventos.Models.Paquete>

@{
    ViewBag.Title = "Index";
}

<h2>Paquetes disponibles</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.idPaquete)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.precio)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.lugar)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.disponible)
        </th>

        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.idPaquete)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.nombre)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.precio)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.lugar)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.disponible.Value)
    </td>

</tr>
}

</table>
